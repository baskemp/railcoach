<!doctype html>
<!--[if lt IE 7 ]> <html class="no-js ie6 exhibition" lang="en"> <![endif]-->
<!--[if IE 7 ]>    <html class="no-js ie7 exhibition" lang="en"> <![endif]-->
<!--[if IE 8 ]>    <html class="no-js ie8 exhibition" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!--> <html class="no-js exhibition" lang="en"> <!--<![endif]-->
  <head>
    <%= render :partial => 'layouts/head' %>
  </head>
  <body>
    <header>
      <%= yield :exhibition %>
      <%= render :partial => 'layouts/navigation' %>
    </header>
    <div class="content gridlayout">
      <div class="breadcrumb">
        <%= yield :breadcrumb %>
      </div>
      <%= yield %>
    </div>
    <footer>
      <%= render :partial => 'layouts/sitemap' %>
      <%= render :partial => 'layouts/toolbar' %>
    </footer>
    <%= javascript_include_tag :after_load %>
  </body>
</html>