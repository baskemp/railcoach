<div class="toolbar">
  <ul class="right">
    <% if @current_user.present? %>
      <li><%= link_to 'Edit Account Information', user_path(@current_user) %></li>
      <li><%= link_to 'Edit Profile', user_profile_path(@current_user) %></li>
      <li><%= link_to 'Logout', logout_path %></li>
    <% else %>
      <li><%= link_to 'Login', login_path %></li>
      <li><%= link_to 'Register', new_user_path %></li>
    <% end %>
  </ul>
</div>
